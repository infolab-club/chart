<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/checkOut"/>
        </shape>
    </item>

    <item
        android:left="3dp"
        android:right="3dp"
        android:top="3dp"
        android:bottom="3dp">
        <shape android:shape="oval">
            <solid android:color="@color/checkInner"/>
        </shape>
    </item>

    <item
        android:left="6dp"
        android:right="6dp"
        android:top="6dp"
        android:bottom="6dp">
        <shape android:shape="oval">
            <solid android:color="@color/checkOn"/>
        </shape>
    </item>
</layer-list>